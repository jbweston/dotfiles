account jbw
host smtp.webfaction.com
port 587
protocol smtp
auth on
passwordeval pass email/jbw
from me@josephweston.org
user jbw
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account gmail
host smtp.gmail.com
port 587
protocol smtp
auth on
from joseph.weston08@gmail.com
user joseph.weston08@gmail.com
passwordeval pass email/gmail
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account tudelft
host smtp-a.tudelft.nl
port 465
protocol smtp
auth on
from j.b.weston@tudelft.nl
user jweston
passwordeval pass email/tudelft
tls on
tls_starttls off
tls_trust_file /etc/ssl/certs/ca-certificates.crt
